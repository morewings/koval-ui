{
  "name": "react-forge-ui",
  "homepage": "https://morewings.github.io/react-forge-ui/",
  "license": "MIT",
  "private": false,
  "version": "0.1.2",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "keywords": [
    "react",
    "template",
    "react-testing-library",
    "testing-library",
    "eslint",
    "npm",
    "rollup",
    "stylelint",
    "library",
    "package",
    "vite",
    "css-modules",
    "typescript"
  ],
  "description": "Missing React, typescript NPM library creation template",
  "scripts": {
    "dev": "vite",
    "clean:lib": "rm -rf dist",
    "build:lib": "vite build",
    "build": "run-s clean:lib build:lib",
    "lint:code": "eslint ./src/ --ext ts,tsx --report-unused-disable-directives",
    "fix:code": "run-s 'lint:code --fix'",
    "lint:types": "tsc --pretty --noEmit",
    "lint:style": "stylelint ./src/**/*.css",
    "fix:style": "run-s 'lint:style --fix'",
    "test": "jest",
    "preview": "vite preview",
    "start:docs": "storybook dev -p 6006 --no-open",
    "build:docs": "storybook build",
    "generate:component": "npx generate-react-cli component",
    "generate:input": "npx generate-react-cli component --type=input",
    "generate:icon": "npx generate-react-cli component --type=icon --flat",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@material-symbols/svg-700": "^0.14.3",
    "awesome-debounce-promise": "^2.1.0",
    "classnames": "^2.5.1",
    "css-vars-hook": "^0.7.00",
    "lodash": "^4.17.21",
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.7",
    "@storybook/addon-interactions": "7.6.7",
    "@storybook/addon-links": "7.6.7",
    "@storybook/addon-storysource": "7.6.7",
    "@storybook/blocks": "7.6.7",
    "@storybook/builder-vite": "7.6.12",
    "@storybook/react": "7.6.7",
    "@storybook/react-vite": "7.6.7",
    "@storybook/test": "7.6.7",
    "@storybook/types": "7.6.7",
    "@testing-library/jest-dom": "6.1.6",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.202",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "@vitejs/plugin-react": "4.2.1",
    "@yelo/rollup-node-external": "^1.0.1",
    "alias-hq": "6.2.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-ssr-friendly": "1.3.0",
    "eslint-plugin-storybook": "0.6.15",
    "generate-react-cli": "8.4.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.32",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.6.7",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "4.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vite-plugin-dts": "3.7.0",
    "vite-plugin-svgr": "4.2.0"
  }
}
